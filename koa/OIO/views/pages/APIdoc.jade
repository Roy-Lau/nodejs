            #ACI-attach
              h3 5. 附:签名算法
              h4 5.1 JAVA
              img(src='../assets/images/java_attach_demo.png', width='1030', alt='java代码示例图')
              h4 5.2 PHP
              pre(data-language='php').
                \n                           public static function sign($params)
                                                    {
                                                        unset($params['sid']);
                                                        unset($params['sign']);
                                                        //按照key进行排序
                                                        ksort($params);
                                                        $signParams = [];
                                                        foreach ($params as $key => $val) {
                                                            $signParams[] = $key . $val;
                                                        }
                                                        $signParams = self::APP_SECRET . implode('', $signParams);
                                                        $sign = strtoupper(md5($signParams));
                                                        return $sign;
                                                    }
